{
  "name": "egyland",
  "version": "1.0.0",
  "description": "Professional platform for an Egyptian agricultural export company built with Django and Tailwind CSS.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "tailwindcss -i ./static/scss/tailwind.scss -o ./static/css/tailwind.min.css --minify",
    "watch:css": "tailwindcss -i ./static/scss/tailwind.scss -o ./static/css/tailwind.min.css --watch",
    "build:scss": "sass --style=compressed --no-source-map ./static/scss:./static/css",
    "watch:scss": "sass --watch --style=compressed --no-source-map ./static/scss:./static/css",
    "dev": "npm run watch:css & npm run watch:scss"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.1.4"
  },
  "dependencies": {
    "browserslist": "^4.24.4",
    "caniuse-lite": "^1.0.30001715",
    "electron-to-chromium": "^1.5.139",
    "escalade": "^3.2.0",
    "fraction.js": "^4.3.7",
    "nanoid": "^3.3.11",
    "node-releases": "^2.0.19",
    "normalize-range": "^0.1.2",
    "picocolors": "^1.1.1",
    "postcss-value-parser": "^4.2.0",
    "source-map-js": "^1.2.1",
    "update-browserslist-db": "^1.1.3"
  }
}
